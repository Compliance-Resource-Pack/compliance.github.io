{
  "name": "website",
  "version": "1.0.0",
  "description": "Faithful Website",
  "main": "index.js",
  "scripts": {
    "ji": "bundle install",
    "jb": "bundle exec jekyll build",
    "start": "npm run ji && npm run jb && pm2 start \"node index.js\" --name \"WEBSITE\"",
    "dev": "concurrently \"npm:dev-*\"",
    "dev-watch-site": "bundle exec jekyll build --watch",
    "dev-watch-app": "node_modules\\.bin\\nodemon index.js --watch index.js --ignore '_site/**'",
    "dev-watch-app-unix": "node_modules/.bin/nodemon index.js --watch index.js --ignore '_site/**'"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Faithful-Resource-Pack/Website.git"
  },
  "keywords": [],
  "author": "",
  "license": "Faithful Resource Pack License",
  "bugs": {
    "url": "https://github.com/Faithful-Resource-Pack/Website/issues"
  },
  "homepage": "https://github.com/Faithful-Resource-Pack/Website#readme",
  "dependencies": {
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.20"
  }
}
